{
  "printWidth": 200,
  "bracketSameLine": true,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "parser": "flow"
      }
    },
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ]
}
